//array sorted or not
#include <iostream>
using namespace std;
bool sorted(int* arr, int n ,int i){
    //base case
    if(i==n-1)
       return true; 
     return (arr[i] <= arr[i + 1]) && sorted(arr, n, i + 1);
}
int main() {
  int n;
  cin>>n;
  int arr[n];
for(int i=0;i<n;i++){
    cin>>arr[i];
}
if(sorted(arr,n,0)){
    cout<<"sorted";
}else{
    cout<<"not sorted";
}
    return 0;
}
